{
    "name" : "seffeng/laravel-helpers",
    "description" : "PHP laravel extension helpers",
    "keywords" : [
        "PHP",
        "laravel",
        "lumen",
        "seffeng",
        "helpers"
    ],
    "license" : "MIT",
    "homepage" : "https://github.com/seffeng/laravel-helpers",
    "authors" : [{
            "name" : "seffeng",
            "email" : "save.zxf@gmail.com",
            "homepage" : "https://github.com/seffeng"
        }
    ],
    "require" : {
        "php" : ">=5.6",
        "illuminate/console": ">=5.0",
        "illuminate/database": ">=5.0",
        "illuminate/encryption": ">=5.0",
        "illuminate/support": ">=5.0",
        "seffeng/arr-helper": ">=0.1.0",
        "seffeng/format-helper": ">=0.1.0",
        "seffeng/json-helper": ">=0.1.0",
        "seffeng/str-helper": ">=0.1.0",
        "seffeng/time-helper": ">=0.1.0",
        "seffeng/url-helper": ">=0.1.0",
        "seffeng/xml-helper": ">=0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0"
    },
    "autoload" : {
        "psr-4" : {
            "Seffeng\\LaravelHelpers\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Seffeng\\LaravelHelpers\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "stable"
}
